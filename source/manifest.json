{
	"name": "Aras Innovator",
	"version": "0.0.1",
	"description": "Aras Innovator browser extension",
	"homepage_url": "https://github.com/anthony-redFox/Innovator-extension",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "awesome-extension@notlmn.github.io",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"tabs", "activeTab", "<all_urls>"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	}
}
